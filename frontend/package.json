        "jwt-decode": "^4.0.0",
        "pinia": "^2.3.0",
        "vue": "^3.3.11",
        "vue-router": "^4.2.5",
        "vuetify": "^3.4.7"
    },
    "devDependencies": {
        "@vitejs/plugin-vue": "^4.5.2",
        "eslint": "^8.49.0",
        "eslint-plugin-vue": "^9.17.0",
        "prettier": "^3.0.3",
